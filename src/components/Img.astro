---
// import { Picture as ImageToolsImg } from 'astro-imagetools/components'

interface Props {
  src: string
}

const props = Astro.props

const url = props.src.startsWith('/') ? props.src : `/${props.src}`
---

<!-- <ImageToolsImg {...props} format={['webp', 'jpg']} placeholder="none" /> -->
<img {...props} src={url} />
